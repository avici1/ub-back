{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["warn", "single"],
        "linebreak-style": 0,
        "import/newline-after-import": 0,
        "arrow-parens":0,
        "eol-last":0,
        "max-len":0,
        " no-else-return": 0,
        " consistent-return" : 0,
         "no-unreachable ": 0,
         "no-useless-catch" : 0,
        " no-plusplus ": 0,
        "no-console":0,
        "consistent-return":0,
        "import/no-named-as-default-member":0,
        "import/no-named-as-default":0,
        "import/no-unresolved":0,
        " no-unused-vars" : 0,
        "brace-style":0,
        "no-else-return":0
    }
}