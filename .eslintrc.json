{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["warn", "single"],
        "linebreak-style": 0,
        "import/newline-after-import": 0,
        "arrow-parens":0,
        "eol-last":0,
        "no-useless-catch":0,
        "max-len":0
    }
}